{
  "name": "capacitor-native-biometric",
  "version": "4.1.3",
  "description": "This plugin gives access to the native biometric apis for android and iOS",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Jose Martinez",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^5.0.3"
  },
  "devDependencies": {
    "@capacitor/android": "^5.0.3",
    "@capacitor/ios": "^5.0.3",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorNativeBiometric.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epicshaggy/capacitor-native-biometric"
  },
  "bugs": {
    "url": "https://github.com/epicshaggy/capacitor-native-biometric/issues"
  }
}
